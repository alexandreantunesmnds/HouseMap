<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ConsultPieceActvity">

    <ImageView
            android:src="@mipmap/ic_launcher"
            android:layout_width="171dp"
            android:layout_height="239dp" android:id="@+id/img_piece"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="68dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="21dp" android:contentDescription="TODO"/>
    <ImageView
            android:src="@mipmap/ic_launcher"
            android:layout_width="171dp"
            android:layout_height="239dp" android:id="@+id/img_piece2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="68dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/img_piece" android:contentDescription="TODO"/>
    <ImageView
            android:src="@mipmap/ic_launcher"
            android:layout_width="171dp"
            android:layout_height="239dp" android:id="@+id/img_piece4"
            android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/img_piece2" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/img_piece3" tools:ignore="ImageContrastCheck"/>
    <ImageView
            android:src="@mipmap/ic_launcher"
            android:layout_width="171dp"
            android:layout_height="239dp" android:id="@+id/img_piece3"
            android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/img_piece" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="21dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>